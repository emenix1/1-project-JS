{
    "name": "@js_ts/webapp",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json ",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
        "preview": "vite preview",
        "stylecheck": "sass --update -q ./src:.node_modules/.cache.stylecheckcache",
        "prettify": "prettier --log-level warn --cache --write \"src/**/*.{ts,tsx,js,jsx,json,yml,yaml}\" \"webapp/**/*.{ts,tsx,js,jsx,json,yml,yaml}\" --ignore-path .gitignore"
    },
    "dependencies": {
        "@js_ts/backend": "workspace:*",
        "@tanstack/react-query": "^5.83.0",
        "@trpc/client": "^11.4.3",
        "@trpc/react-query": "^11.4.3",
        "classnames": "^2.5.1",
        "date-fns": "^4.1.0",
        "formik": "^2.4.6",
        "formik-validator-zod": "^2.2.0",
        "include-media": "^2.0.0",
        "js-cookie": "^3.0.5",
        "lodash": "^4.17.21",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.7.0",
        "reset-css": "^5.0.2",
        "sass": "^1.89.2",
        "zod": "^4.0.10"
    },
    "devDependencies": {
        "@types/js-cookie": "^3.0.6",
        "@types/lodash": "^4.17.20",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.6.0",
        "globals": "^16.3.0",
        "vite": "^7.0.4"
    }
}
